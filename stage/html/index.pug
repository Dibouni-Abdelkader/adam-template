
-var title = 'Adam Template'
doctype html
html(lang='en')
	head
		//- Include Meta Tags From meta.pug
		include pug/includes/meta.pug

		//- Title 
		title= title

		//- Include Styles From styles.pug
		include pug/includes/styles.pug
	body
		//- Include Components
		include pug/components/switch-theme.pug
		include pug/components/preloader.pug
		include pug/components/packtop.pug
		include pug/components/section-title.pug
		
		//-Include Sections From Layout
		include pug/layout/navigation.pug
		include pug/layout/hero.pug
		include pug/layout/about.pug
		include pug/layout/services.pug
		include pug/layout/portfolio.pug
		include pug/layout/testimonials.pug
		include pug/layout/price.pug
		include pug/layout/blog.pug
		include pug/layout/contact.pug
		include pug/layout/footer.pug
		//- Include Scripts From scripts.pug
		include pug/includes/scripts.pug




		
